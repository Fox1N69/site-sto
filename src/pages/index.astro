---
import Layout from "../layouts/Layout.astro";

---

<Layout title="Title page">
  <main class="flex flex-col items-center pb-20 bg-neutral-900">
    <section
      class="flex flex-col items-center self-stretch px-12 pt-px pb-20 w-full text-center bg-neutral-900 max-md:px-5 max-md:max-w-full"
    >
      <header
        class="flex gap-5 justify-between items-center self-stretch w-full max-md:flex-wrap max-md:max-w-full"
      >
        <img
          src="https://cdn.builder.io/api/v1/image/assets/TEMP/c0351d670025db3be3399879186aec361c532c7220ea8e0f6f1497855dd516b3?apiKey=86a8077d13b044688be2a49a34f38b33&"
          alt="Company Logo"
          class="shrink-0 self-stretch max-w-full mix-blend-lighten aspect-[1.75] w-[209px]"
        />
        <nav
          class="flex gap-5 justify-between self-stretch my-auto text-base font-light leading-4 whitespace-nowrap max-md:flex-wrap max-md:max-w-full"
        >
          <a href="#" class="font-semibold bg-clip-text">Акции</a>
          <a href="#" class="text-white">Услуги</a>
          <a href="#" class="font-extralight text-white">Отзывы</a>
          <a href="#" class="text-white">Контакты</a>
        </nav>
        <div class="self-stretch my-auto text-xl leading-5 text-white">
          9.00-20.00 пн-пт
        </div>
      </header>
      <img
        src="https://cdn.builder.io/api/v1/image/assets/TEMP/86d9fe4a80e25a78f2c5dd6a87b9c5d4e0b9b6abff47e107ad91a8a112a66a7e?apiKey=86a8077d13b044688be2a49a34f38b33&"
        alt="Promotional Banner"
        class="mt-12 max-w-full aspect-[3.23] w-[407px] max-md:mt-10"
      />
      <a
        href="#"
        class="justify-center items-center px-16 py-5 mt-11 mb-80 max-w-full text-lg font-semibold tracking-widest leading-5 text-white whitespace-nowrap bg-red-600 rounded-md shadow-lg w-[204px] max-md:px-5 max-md:my-10"
        >Запись</a
      >
    </section>

    <section>
      <img
        src="https://cdn.builder.io/api/v1/image/assets/TEMP/0fa06b43789abad72a5f015b0988f05c299dd606c7b1c1048a9aaada69a2c61d?apiKey=86a8077d13b044688be2a49a34f38b33&"
